= form_for @migration do |f|
  - if @migration.errors.any?
    #error_explanation
      %h2= "#{pluralize(@migration.errors.count, "error")} prohibited this migration from being saved:"
      %ul
        - @migration.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :RemoveCompanyFromContractors
    = f.text_field :RemoveCompanyFromContractors
  .field
    = f.label :company_id
    = f.number_field :company_id
  .actions
    = f.submit 'Save'
