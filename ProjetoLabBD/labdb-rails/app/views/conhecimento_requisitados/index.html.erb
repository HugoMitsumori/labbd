<p id="notice"><%= notice %></p>

<h1>Conhecimento Requisitados</h1>

<table>
  <thead>
    <tr>
      <th>Nome conhecimento</th>
      <th>Codigo servico</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conhecimento_requisitados.each do |conhecimento_requisitado| %>
      <tr>
        <td><%= conhecimento_requisitado.nome_conhecimento %></td>
        <td><%= conhecimento_requisitado.codigo_servico %></td>
        <td><%= link_to 'Show', conhecimento_requisitado %></td>
        <td><%= link_to 'Edit', edit_conhecimento_requisitado_path(conhecimento_requisitado) %></td>
        <td><%= link_to 'Destroy', conhecimento_requisitado, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conhecimento Requisitado', new_conhecimento_requisitado_path %>
